<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="CustomClockView">
        <attr name="second_hand_color" format="color" />
        <attr name="minute_hand_color" format="color" />
        <attr name="hour_hand_color" format="color" />
        <attr name="clock_color" format="color" />

        <attr name="second_start_time" format="integer" />
        <attr name="minute_start_time" format="integer" />
        <attr name="hour_start_time" format="integer" />


    </declare-styleable>
    <attr name="customClockStyle" format="reference" />
</resources>
